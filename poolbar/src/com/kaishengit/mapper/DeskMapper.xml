<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kaishengit.mapper.DeskMapper">
	<cache/>	
	
	<select id="findById" resultType="Desk" parameterType="int">
		select * from t_desk where id = #{id}
	</select>
	
	<insert id="save" parameterType="Desk">
		insert into t_desk(id,price,barid,orderid) values(#{id},#{price},#{barid},#{orderid}) 
	</insert>
	
	<update id="update" parameterType="Desk">
		update t_desk set price=#{price},barid = #{barid},orderid = #{orderid} where id = #{id}
	</update>
	
	<delete id="del" parameterType="String">
		delete from t_desk where id = #{id}
	</delete>
</mapper>