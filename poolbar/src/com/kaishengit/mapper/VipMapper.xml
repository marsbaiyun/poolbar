<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kaishengit.mapper.VipMapper">
	<cache/>	
	
	<select id="findById" resultType="Vip" parameterType="String">
		select * from t_vip where id = #{id}
	</select>
	
	<insert id="save" parameterType="Vip">
		insert into t_vip(id,`name`,password,tel,discount) values(#{id},#{name},#{password},#{tel},#{discount}) 
	</insert>
	
	<update id="update" parameterType="Vip">
		update t_vip set `name` = #{name},password = #{password},tel = #{tel},discount = #{discount} where id = #{id}
	</update>
	
	<delete id="del" parameterType="String">
		delete from t_vip where id = #{id}
	</delete>
</mapper>